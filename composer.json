{
    "name": "i4erkasov/phalcon-xml-annotations",
    "description": "Phalcon XML Annotations",
    "type": "library",
    "keywords": ["phalcon","xml","annotations","phalcon xml","phalcon xml annotation","object to xml","phalcon object to xml"],
    "descriptions": "Phalcon XML Annotations is a library that allows you to convert PHP objects into XML files. It provides annotations to define the structure and parameters of XML files based on PHP objects.",
    "license": "MIT",
    "authors": [
        {
            "name": "ivan cherkasov",
            "email": "i.4erkasov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-psr": "^1",
        "ext-xmlwriter": "*",
        "ext-phalcon": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "I4\\Phalcon\\XML\\": "src",
            "TestPhalconXML\\": "tests",
            "ExamplePhalconXML\\": "example"
        }
    }
}
